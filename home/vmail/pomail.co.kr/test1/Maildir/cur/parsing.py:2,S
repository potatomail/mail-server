import datetime
import json
import eml_parser


def json_serial(obj):
  if isinstance(obj, datetime.datetime):
      serial = obj.isoformat()
      return serial

with open('\'1616309617.M697022P6952.mail.pomail.co.kr,S=15162,W=15384_2,S.eml\'', 'rb') as fhdl:
  raw = fhdl.read()

ep = eml_parser.EmlParser()
parsed_eml = ep.decode_email_bytes(raw)
#parsed = ep.parse_email()

print(parsed_eml)
print("-----------------------------------------------------------------")
